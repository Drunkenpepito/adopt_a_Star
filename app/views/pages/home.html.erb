<div class= "container col-5">
  <h1>Welcome to Adopt a Star</h1>
  <div class="form">
    <%= simple_form_for :search, url: stars_path, method: :get do |f| %>
      <%= f.input :city %>
      <%= f.input :category %>
      <%= f.button :submit, class: "btn btn-gradient"%>
    <% end %>
  </div>
</div>
