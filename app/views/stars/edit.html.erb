<div class= "container col-5">
  <h1>Edit Star's profile</h1>
  <div class="form">
    <%= simple_form_for(@star) do |f| %>
      <%= f.input :name %>
      <%= f.input :city %>
      <%= f.input :category %>
      <%= f.input :price %>
      <%= f.input :sex %>
      <%= f.input :size %>
      <%= f.input :photo, as: :file %>
      <%= f.button :submit, class: "btn btn-gradient"%>
    <% end %>
  </div>
</div>
